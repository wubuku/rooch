<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.test.roochblogdemo.domain.article">
  <class name="org.test.roochblogdemo.rooch.contract.persistence.CommentTableItemAdded" table="rooch_comment_table_item_added">
    <composite-id name="articleCommentId" class="org.test.roochblogdemo.domain.article.ArticleCommentId">
      <key-property name="articleId">
        <column name="article_comment_id_article_id" sql-type="VARCHAR(66)" />
      </key-property>
      <key-property name="commentSeqId">
        <column name="article_comment_id_comment_seq_id" sql-type="DECIMAL(20,0)" />
      </key-property>
    </composite-id>
    <component name="roochEventId" class="org.test.roochblogdemo.domain.RoochEventId">
      <property name="eventHandleId">
        <column name="rooch_event_id_event_handle_id" sql-type="VARCHAR(66)" />
      </property>
      <property name="eventSeq">
        <column name="rooch_event_id_event_seq" sql-type="DECIMAL(20,0)" />
      </property>
    </component>
    <property name="roochSender">
      <column name="rooch_sender" sql-type="VARCHAR(66)" />
    </property>
    <property name="roochTxHash">
      <column name="rooch_tx_hash" sql-type="VARCHAR(66)" />
    </property>
    <property name="roochTypeTag">
      <column name="rooch_type_tag" length="500" />
    </property>
    <property name="roochTimestampMs">
      <column name="rooch_timestamp_ms" />
    </property>
    <property name="roochBlockHeight">
      <column name="rooch_block_height" sql-type="DECIMAL(20,0)" />
    </property>
    <property name="roochEventIndex">
      <column name="rooch_event_index" />
    </property>
    <property name="createdBy">
      <column name="created_by" />
    </property>
    <property name="updatedBy">
      <column name="updated_by" />
    </property>
    <property name="deleted">
      <column name="deleted" />
    </property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>
</hibernate-mapping>
